    "cors": "^2.8.5",
    "cracksdb": "^0.1.0",
    "expo": "^51.0.14",
    "expo-image": "^1.12.9",
    "expo-status-bar": "^1.12.1",
    "express": "^4.19.2",
    "react": "18.2.0",
    "react-native": "^0.74.3",
    "react-native-paper": "^5.12.3",
    "react-native-safe-area-context": "^4.10.1",
    "react-responsive": "^10.0.0"

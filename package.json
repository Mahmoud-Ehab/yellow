{
  "name": "yellow",
  "version": "0.0.1",
  "description": "A cross-platform peer-to-peer chat application made with javascript.",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start-expo": "expo start",
    "start-electron": "electron resources/js/electron.js --dev",
    "compile": "rollup --config",
    "compile-dev": "rollup --config --watch",
    "build-web": "expo export --platform web --output-dir resources/web",
    "build-exe-try": "node ./forge.build.js start",
    "build-exe": "node ./forge.build.js make",
    "build-android": "expo run:android",
    "build-ios": "expo run:ios"
  },
  "dependencies": {
    "@expo/match-media": "^0.4.0",
    "@types/react": "^18.2.79",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "cracksdb": "^1.0.1",
    "electron-squirrel-startup": "^1.0.1",
    "expo": "^51.0.22",
    "expo-image": "^1.12.13",
    "expo-image-picker": "~15.0.7",
    "expo-status-bar": "^1.12.1",
    "express": "^4.19.2",
    "react": "18.2.0",
    "react-native": "^0.74.3",
    "react-native-paper": "^5.12.3",
    "react-native-safe-area-context": "^4.10.5",
    "react-responsive": "^10.0.0",
    "serve-handler": "^6.1.5",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
    "@expo/metro-runtime": "^3.2.1",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "electron": "^31.3.0",
    "nodemon": "^3.1.3",
    "prettier": "^3.3.3",
    "react-dom": "^18.2.0",
    "react-native-web": "^0.19.10",
    "rollup": "^4.13.0",
    "tsc": "^2.0.4",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mahmoud-Ehab/yellow.git"
  },
  "keywords": [
    "chat",
    "javascript",
    "typescript",
    "electron",
    "nodejs"
  ],
  "author": "M. E. Abdelsalam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mahmoud-Ehab/yellow/issues"
  },
  "homepage": "https://github.com/Mahmoud-Ehab/yellow#readme"
}
